public class TestaVariaveis {

		public static void main(String[] args) {
			
			int idade = 24;
			System.out.println("Novo teste");	
			System.out.println("Idade: "+idade+" anos");
			
			double valor = 7.5;
			int total = (int) valor;
			System.out.println();
			System.out.println(total);
			
		}
}
